<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">

	<route method="GET" url="/V1/elightwalk_razorpay/getrzpdata/">
		<service class="Elightwalk\Razorpay\Api\CodeInterface" method="getRzpConfig"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>

	<route method="POST" url="/V1/elightwalk_razorpay/setrzpdata/">
		<service class="Elightwalk\Razorpay\Api\CodeInterface" method="setRzpData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>

</routes>
